#!/bin/bash
#build file for Unix system
currentDir=`dirname $0`
if [ $# -lt 1 ]
then
  args="-projecthelp"
else
  args=$@
fi
ANT_HOME=$currentDir $JAVA_HOME/bin/java  -Dant.home=\"$currentDir\" -Dant.library.dir=\"$currentDir/lib\" -Xmx256000000 -classpath $currentDir/lib/ant-launcher.jar org.apache.tools.ant.launch.Launcher -buildfile  $currentDir/proactive.xml $args
