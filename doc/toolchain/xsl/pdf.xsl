		<xsl:choose>
			<xsl:when test="count($label)&gt;0">
				<xsl:apply-templates select="$label">
				</xsl:apply-templates>
			</xsl:when>
			<xsl:when test="$deflabel = 'number' and local-name(.) = 'question'">
				<xsl:value-of select="$prefix">
				</xsl:value-of>
				<xsl:number count="qandaentry" format="1" level="multiple">
				</xsl:number>
			</xsl:when>
		</xsl:choose>
	</xsl:template>
