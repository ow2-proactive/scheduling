						<xsl:apply-templates mode="label.markup" select="ancestor::qandadiv[1]">
						</xsl:apply-templates>
						<xsl:apply-templates mode="intralabel.punctuation" select="ancestor::qandadiv[1]">
						</xsl:apply-templates>
