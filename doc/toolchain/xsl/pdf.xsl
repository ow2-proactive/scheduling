			<xsl:value-of select="(ancestor-or-self::*[@defaultlabel])[last()]/@defaultlabel">
			</xsl:value-of>
		</xsl:variable>
		<fo:block id="{$entry.id}">
			<xsl:choose>
				<xsl:when test="$deflabel = 'none'">
					<xsl:message>
						<xsl:text>
							qandaentry: 'answer' has no text!
						</xsl:text>
						<xsl:copy-of select=".">
