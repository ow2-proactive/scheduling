	<!-- //FIXME  change ulink to uri -->
	<!-- copy of xref.xsl, line 783. The link on [url] is also clickeable-->
	<xsl:template match="ulink" name="ulink">
		<fo:basic-link xsl:use-attribute-sets="xref.properties">
