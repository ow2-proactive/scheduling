<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE application PUBLIC "-//Sun Microsystems, Inc.//DTD J2EE Application 1.3//EN" "http://java.sun.com/dtd/application_1_3.dtd">

<application id="Proactive">
    <display-name>Proactive</display-name>
    <description>Proactive with all its underlying components</description>

    <module id="Proactive_Server">
        <web>
            <web-uri>proactive-server.war</web-uri>
            <context-root>/proactive</context-root>
        </web>
    </module>

   <module id="Proactive_GetStarted">
        <web>
            <web-uri>proactive-getstarted.war</web-uri>
            <context-root>/</context-root>
        </web>
    </module>

    <module id="Proactive_Rest">
        <web>
            <web-uri>proactive-rest.war</web-uri>
            <context-root>/rest</context-root>
        </web>
    </module>

    <module id="Proactive_RM">
        <web>
            <web-uri>proactive-rm.war</web-uri>
            <context-root>/rm</context-root>
        </web>
    </module>

    <module id="Proactive_Scheduler">
        <web>
            <web-uri>proactive-scheduler.war</web-uri>
            <context-root>/scheduler</context-root>
        </web>
    </module>

    <module id="Proactive_Studio">
        <web>
            <web-uri>proactive-studio.war</web-uri>
            <context-root>/studio</context-root>
        </web>
    </module>

    <module id="Proactive_Workflow_Catalog">
        <web>
            <web-uri>proactive-workflow-catalog.war</web-uri>
            <context-root>/workflow-catalog</context-root>
        </web>
    </module>

    <module id="Proactive_Scheduling_API">
        <web>
            <web-uri>proactive-scheduling-api.war</web-uri>
            <context-root>/scheduling-api</context-root>
        </web>
    </module>

</application>

