<?xml version="1.0" encoding="UTF-8"?>
<job
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="urn:proactive:jobdescriptor:3.4"
        xsi:schemaLocation="urn:proactive:jobdescriptor:3.4 http://www.activeeon.com/public_content/schemas/proactive/jobdescriptor/3.4/schedulerjob.xsd"
        name="WebNotification" projectName="Notifications"
        priority="normal"
        onTaskError="continueJobExecution"
        maxNumberOfExecution="2"
>
  <description>
    <![CDATA[ Job to send a message to a notification service. ]]>
  </description>
  <taskFlow>
    <task name="WebNotification"
    >
      <description>
        <![CDATA[ Task to send a message to a notification service. ]]>
      </description>
      <javaExecutable class="org.ow2.proactive.addons.webhook.task.WebhookTask">
        <parameters>
          <parameter name="User-Agent" value="Mozilla/5.0"/>
          <parameter name="method" value="GET"/>
          <parameter name="url" value="http://www.google.com/search?q=mkyong"/>
        </parameters>
      </javaExecutable>
    </task>
  </taskFlow>
</job>