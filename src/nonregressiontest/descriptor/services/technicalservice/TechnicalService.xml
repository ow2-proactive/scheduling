<?xml version="1.0" encoding="UTF-8"?>
<ProActiveDescriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www-sop.inria.fr/oasis/proactive/schema/3.2/DescriptorSchema.xsd">
	<!-- <security file="../../descriptors/security/c3dPolicy.xml"></security> -->
	<componentDefinition>
		<virtualNodesDefinition>
			<virtualNode name="VN1" property="unique_singleAO" technicalServiceId="service1"/>
			<virtualNode name="VN2" property="unique_singleAO" technicalServiceId="service2"/>
		</virtualNodesDefinition>
	</componentDefinition>
	<deployment>
		<mapping>
			<map virtualNode="VN1">
				<jvmSet>
					<vmName value="Jvm1"/>
				</jvmSet>
			</map>
			<map virtualNode="VN2">
				<jvmSet>
					<vmName value="Jvm2"/>
				</jvmSet>
			</map>
		</mapping>
		<jvms>
			<jvm name="Jvm1">
				<creation>
					<processReference refid="localJVM"/>
				</creation>
			</jvm>
			<jvm name="Jvm2">
				<creation>
					<processReference refid="localJVM"/>
				</creation>
			</jvm>
		</jvms>
	</deployment>
	<infrastructure>
		<processes>
			<processDefinition id="localJVM">
				<jvmProcess class="org.objectweb.proactive.core.process.JVMNodeProcess"></jvmProcess>
			</processDefinition>
		</processes>
	</infrastructure>
	<technicalServices>
		<technicalServiceDefinition id="service1" class="nonregressiontest.descriptor.services.technicalservice.TS">
			<arg name="arg1" value="aaa"/>
		</technicalServiceDefinition>
		<technicalServiceDefinition id="service2" class="nonregressiontest.descriptor.services.technicalservice.TS">
			<arg name="arg1" value="bbb"/>
			<arg name="arg2" value="ccc"/>
		</technicalServiceDefinition>
	</technicalServices>
</ProActiveDescriptor>
