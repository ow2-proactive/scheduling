		<virtualNodesDefinition>
			<virtualNode name="VnTest" property="multiple"/>
		</virtualNodesDefinition>
	</componentDefinition>
	<deployment>
		<mapping>
			<map virtualNode="VnTest">
				<jvmSet>
					<vmName value="Jvm1"/>
				</jvmSet>
			</map>
		</mapping>
		<jvms>
			<jvm name="Jvm1">
