			</jvm>
		</jvms>
	</deployment>
	<infrastructure>
		<services>
			<serviceDefinition id="lookup">
				<RMIRegistryLookup url="//localhost:3020/PA_JVM1"></RMIRegistryLookup>
			</serviceDefinition>
		</services>
	</infrastructure>
