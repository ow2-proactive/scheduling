<?xml version="1.0" encoding="UTF-8"?>
<GCMDeployment 
	xmlns="urn:gcm:deployment:1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:gcm:deployment:1.0 http://proactive.inria.fr/schema/gcm/1.0/ExtensionSchemas.xsd	">

	<resources>
		<group refid="mygroup">
			<host refid="WRONG_ID" />
		</group>
	</resources>

	<infrastructure>
	
		<groups>
			<sshGroup id="mygroup" hostList="host" />
		</groups>

		<hosts>
			<host id="myhost" os="unix">
				<homeDirectory base="root" relpath="" />
			</host>
		</hosts>

	</infrastructure>

</GCMDeployment>
